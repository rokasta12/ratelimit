{
  "name": "@jfungus/ratelimit-nuxt",
  "version": "1.0.0",
  "description": "Nuxt module for @jfungus/ratelimit - rate limiting with Nuxt's useStorage",
  "author": "rokasta12",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rokasta12/ratelimit.git",
    "directory": "packages/nuxt"
  },
  "homepage": "https://rokasta12.github.io/ratelimit/",
  "bugs": {
    "url": "https://github.com/rokasta12/ratelimit/issues"
  },
  "keywords": ["nuxt", "nuxt-module", "rate-limit", "rate-limiter", "middleware"],
  "type": "module",
  "main": "./dist/module.cjs",
  "module": "./dist/module.js",
  "types": "./dist/module.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/module.d.ts",
        "default": "./dist/module.js"
      },
      "require": {
        "types": "./dist/module.d.cts",
        "default": "./dist/module.cjs"
      }
    }
  },
  "files": ["dist", "runtime", "runtime-js", "README.md"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@jfungus/ratelimit": "workspace:*",
    "@jfungus/ratelimit-unstorage": "workspace:*",
    "@nuxt/kit": "^3.17.4",
    "defu": "^6.1.4"
  },
  "devDependencies": {
    "@nuxt/schema": "^3.21.0",
    "nuxt": "^3.17.4",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "nuxt": "^3.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
