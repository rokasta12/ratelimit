{
  "name": "@jfungus/ratelimit-express",
  "version": "0.0.1",
  "description": "Express middleware for @jfungus/ratelimit",
  "author": "rokasta12",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rokasta12/ratelimit.git",
    "directory": "packages/express"
  },
  "homepage": "https://rokasta12.github.io/ratelimit/",
  "bugs": {
    "url": "https://github.com/rokasta12/ratelimit/issues"
  },
  "keywords": ["express", "rate-limit", "rate-limiter", "middleware"],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist", "src"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@jfungus/ratelimit": "workspace:*"
  },
  "peerDependencies": {
    "express": ">=4.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/supertest": "^6.0.2",
    "express": "^5.1.0",
    "supertest": "^7.1.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18"
  }
}
