{
  "name": "@jfungus/ratelimit-h3",
  "version": "0.0.1",
  "description": "H3/Nitro middleware for @jfungus/ratelimit",
  "author": "rokasta12",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rokasta12/ratelimit.git",
    "directory": "packages/h3"
  },
  "homepage": "https://rokasta12.github.io/ratelimit/",
  "bugs": {
    "url": "https://github.com/rokasta12/ratelimit/issues"
  },
  "keywords": ["h3", "nitro", "nuxt", "rate-limit", "middleware"],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist", "src", "README.md"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@jfungus/ratelimit": "workspace:*"
  },
  "peerDependencies": {
    "h3": ">=1.10.0"
  },
  "devDependencies": {
    "h3": "^1.15.3",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18"
  }
}
