{
  "name": "@jfungus/ratelimit",
  "version": "0.0.1",
  "description": "Framework-agnostic rate limiting library with sliding window algorithm",
  "author": "rokasta12",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rokasta12/ratelimit.git",
    "directory": "packages/core"
  },
  "homepage": "https://rokasta12.github.io/ratelimit/",
  "bugs": {
    "url": "https://github.com/rokasta12/ratelimit/issues"
  },
  "keywords": [
    "rate-limit",
    "rate-limiter",
    "ratelimit",
    "throttle",
    "sliding-window",
    "fixed-window"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist", "src"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18"
  }
}
